    <style name="Theme.Yunda" parent="Theme.MaterialComponents.DayNight.Bridge">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimarySurface</item>
        <item name="background">@drawable/ic_yunda </item>
        <!-- Customize your theme here. -->
    </style>


    <style name="Theme.Yunda.Pendding">

        <item name="background">@drawable/ic_yunda </item>
    </style>

    <style name="Theme.Yunda.DetailTitle" parent="Theme.MaterialComponents.DialogWhenLarge">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>

    </style>
